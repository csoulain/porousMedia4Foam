FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "2400";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0.0001;

boundaryField
{
    outlet
    {
        type            calculated;
        value           uniform 0.0001;
    }
    inlet
    {
        type            calculated;
        value           uniform -0.0001;
    }
    frontAndBack
    {
        type            empty;
        value           nonuniform 0();
    }
}


