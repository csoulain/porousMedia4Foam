	reducedSaturationModel	minMax;
	minMaxCoeffs
	{
		Sbmin		Sbmin [0  0  0 0 0 0 0]	1e-4;
		Sbmax		Sbmax [0  0  0 0 0 0 0]	0.9999;
	}

	heterogeneousScalarConstantCoeffs
	{
		  K0 K0 [0 2 0 0 0 0 0] 1e-11;
	}

	//Sbmin	in constant/porousModels ??
	//Sbmin		Sbmin [0  0  0 0 0 0 0]	1e-4;
