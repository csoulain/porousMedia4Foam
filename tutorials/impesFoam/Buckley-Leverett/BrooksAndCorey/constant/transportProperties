{
  rho	rho [1 -3 0 0 0 0 0] 	1e0;
  mu	mu [1 -1 -1 0 0 0 0]	1.76e-5;
}
	
phase.b
{
  rho	rho [1 -3 0 0 0 0 0] 	1e3;
  mu	mu [1 -1 -1 0 0 0 0]	1e-3;
}



porousMediaProperties
{

	eps		eps [0  0  0 0 0 0 0]	0.5;	

