FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Y.Cl;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 9.97e-16;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.01095;
    }
    frontAndBack
    {
        type            empty;
    }
}


