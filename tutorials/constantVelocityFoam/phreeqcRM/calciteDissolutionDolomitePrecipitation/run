#Script to mesh, run the benchmark case (B1. M Xie - Calcite dissolution), post process output data, plot results



echo Building mesh
blockMesh > log.blockMesh

echo Running the case
constantVelocityFoam > log.constantVelocityFoam
echo End of the run



#Output data at 20, 40 and 60 min.
postProcess -time 1200,2400,3600 -func graphCell

gnuplot plot.gp

echo END
