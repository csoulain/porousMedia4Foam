FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ci;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    right
    {
        type            zeroGradient;
    }
    left
    {
        type            fixedValue;
	value		uniform 1;
    }
    emptyFaces
    {
        type            empty;
    }
}


