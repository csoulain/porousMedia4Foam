FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     darcyFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         4.75e9;

deltaT          21600; //21600;

writeControl    runTime;

writeInterval   1.5e7;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


